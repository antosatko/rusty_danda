entry [expressions
	expression ? set="expressions"
	; ? back="1"
	"'any" ? err="mas tu neco navic";


expression [tokens
	value ? set="tokens" back="0"
	operator ? set="tokens" back="1"
	parenthesis ? set="tokens" back="2";

operator op
	+ ? set="op" end="true"
	- ? set="op" end="true"
	* ? set="op" end="true"
	/ ? set="op" end="true"
	% ? set="op" end="true"
	"'any" err="not an op";

parenthesis [expression
	( harderr="true"
	expression ? set="expression"
	, ? back="1"
	);

value [value
	"'number" ? set="value" end="true"
	"'string" ? set="value" end="true"
	* ? set="value" back="0"
	"'text" set="value"
	nested ? set="value" back="0"
	idx ? set="value" back="1"
	parenthesis ? set="value" back="2"
	"'any";

nested identifier
	. harderr="true"
	"'text" set="identifier";

idx value
	[ harderr="true"
	expression set="value"
	];
